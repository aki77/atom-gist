{
  "name": "gist",
  "main": "./lib/main",
  "version": "0.3.1",
  "description": "create and insert Gists.",
  "keywords": [
    "github",
    "gist"
  ],
  "activationCommands": {
    "atom-text-editor:not([mini])": [
      "gist:create-public",
      "gist:create-private",
      "gist:list"
    ]
  },
  "repository": "https://github.com/aki77/atom-gist",
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "dependencies": {
    "@aki77/atom-input-dialog": "^0.1.3",
    "@aki77/atom-select-action": "^0.1.2",
    "atom-space-pen-views": "^2.0.5",
    "bluebird": "^3.1.4",
    "fuzzaldrin": "^2.1.0",
    "octonode": "^0.7.1",
    "temp": "~0.8.1",
    "underscore-plus": "^1.6.6",
    "untildify": "^3.0.0"
  },
  "configSchema": {
    "token": {
      "order": 1,
      "type": "string",
      "default": "",
      "description": "github personal access token"
    },
    "tokenFile": {
      "order": 2,
      "type": "string",
      "default": "~/.atom/gist.token",
      "description": "not save the token to the configuration file"
    },
    "environmentName": {
      "order": 3,
      "type": "string",
      "default": "GIST_ACCESS_TOKEN",
      "description": "not save the token to the configuration file"
    },
    "closeOnSave": {
      "order": 4,
      "type": "boolean",
      "default": false,
      "description": "close the tab when you save a gist"
    },
    "debug": {
      "order": 99,
      "type": "boolean",
      "default": false
    }
  }
}
